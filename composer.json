{
    "name": "kamelher/devices",
    "description": "A Laravel package for managing devices related to each model that can be authenticated",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kamelher\\Devices\\": "src/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamelher\\Devices\\": "tests/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kamelher\\Devices\\DeviceManagerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php artisan test --parallel --coverage"
        ]
    },
    "authors": [
        {
            "name": "KamelEddine Heragmi"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {

    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {

    }
}
